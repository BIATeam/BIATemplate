{
  "name": "biatemplate",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start4iis": "ng serve --configuration iis",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "e2e": "ng e2e",
    "clean": "npx prettier --write . --plugin=prettier-plugin-organize-imports && ng lint --fix",
    "build:iis": "ng build --configuration iis",
    "build:aot": "ng build --aot --base-href /BIATemplate/App/ --configuration iis",
    "build:aot2": "ng build --aot --base-href /BIATemplate2/App/ --output-path dist/BIATemplate2 --configuration iis",
    "build:prod": "ng build --configuration production --base-href /BIATemplate/App/",
    "build:prod2": "ng build --configuration production --base-href /BIATemplate2/App/ --output-path dist/BIATemplate2",
    "build:ocp": "ng build --configuration production,ocp",
    "build:rosa": "ng build --configuration production,rosa",
    "build:docker": "ng build --configuration docker",
    "deploy": "ng deploy --base-href=/BIATemplate/",
    "styles": "dart compile-sass.dart ./src/assets/bia/primeng/bia/layout-primeng-bia.scss ./src/assets/bia/primeng/layout/styles/layout/layout.css --output-style compressed",
    "stats-compute": "ng build --configuration production --base-href /BIATemplate/App/ --stats-json",
    "stats-display": "webpack-bundle-analyzer --port 4299 dist/BIATemplate/stats.json",
    "stats": "npm run stats-compute && npm run stats-display"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~19.2.15",
    "@angular/cdk": "~19.2.19",
    "@angular/common": "~19.2.15",
    "@angular/compiler": "~19.2.15",
    "@angular/core": "~19.2.15",
    "@angular/forms": "~19.2.15",
    "@angular/platform-browser": "~19.2.15",
    "@angular/platform-browser-dynamic": "~19.2.15",
    "@angular/router": "~19.2.15",
    "@angular/service-worker": "~19.2.15",
    "@microsoft/signalr": "~8.0.7",
    "@ngrx/effects": "~19.2.1",
    "@ngrx/entity": "~19.2.1",
    "@ngrx/operators": "~19.2.1",
    "@ngrx/store": "~19.2.1",
    "@ngx-translate/core": "~16.0.4",
    "@primeng/themes": "~19.1.3",
    "@types/file-saver": "~2.0.7",
    "date-fns": "~4.1.0",
    "deepmerge": "~4.3.1",
    "dexie": "~4.2.1",
    "file-saver": "~2.0.5",
    "jwt-decode": "~4.0.0",
    "keycloak-angular": "~19.0.2",
    "keycloak-js": "~26.2.1",
    "ngx-logger": "~5.0.12",
    "papaparse": "~5.5.3",
    "primeflex": "~4.0.0",
    "primeicons": "~7.0.0",
    "primeng": "~19.1.3",
    "rxjs": "~7.8.2",
    "zone.js": "~0.15.1"
  },
  "devDependencies": {
    "@angular-eslint/builder": "~19.8.1",
    "@angular-eslint/eslint-plugin": "~19.8.1",
    "@angular-eslint/eslint-plugin-template": "~19.8.1",
    "@angular-eslint/schematics": "~19.8.1",
    "@angular-eslint/template-parser": "~19.8.1",
    "@angular/build": "~19.2.18",
    "@angular/cli": "~19.2.18",
    "@angular/compiler-cli": "~19.2.15",
    "@angular/language-service": "~19.2.15",
    "@ngrx/store-devtools": "~19.2.1",
    "@types/node": "~24.8.1",
    "@types/papaparse": "~5.3.16",
    "@typescript-eslint/eslint-plugin": "~8.46.1",
    "@typescript-eslint/parser": "~8.46.1",
    "eslint": "~9.37.0",
    "eslint-config-prettier": "~10.1.5",
    "eslint-plugin-prettier": "~5.5.1",
    "prettier": "~3.6.2",
    "prettier-eslint": "~16.4.2",
    "prettier-plugin-organize-imports": "~4.3.0",
    "ts-node": "~10.9.2",
    "tslib": "~2.8.1",
    "typescript": "~5.8.3"
  }
}
